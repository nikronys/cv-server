    "@types/express": "^4.16.0",
    "@types/passport": "^0.4.7",
    "@types/sequelize": "^4.27.30",
    "concurrently": "^4.0.1",
    "nodemon": "^1.18.6",
    "tslint": "^5.11.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "express": "^4.16.0",
    "module-alias": "^2.1.0",
    "moment": "^2.22.2",
    "passport": "^0.4.0",
    "sequelize": "^4.41.2"
  }
